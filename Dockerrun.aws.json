{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "cat-cluster",
      "image": "320942041258.dkr.ecr.ap-northeast-1.amazonaws.com/cat-cluster:latest",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-cat-cluster",
          "containerPath": "/opt/cat-cluster/log"
        }
      ]
    }
  ]
}
